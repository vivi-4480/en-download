on":0},{"id":180102,"lipSynch":1,"motion":200}]},{"autoTurnFirst":3.5,"chara":[{"id":180100},{"eyeClose":1,"id":180102,"motion":1}]}],"group_15":[{"autoTurnFirst":1.7,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180100,"lipSynch":1,"motion":200,"pos":0,"tear":0,"voice":"vo_char_1801_00_22"},{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":180102,"lipSynch":0,"motion":1,"pos":1,"tear":0,"voice":"vo_char_1801_00_22","zOrder":-1}]},{"autoTurnFirst":3,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180100}]},{"autoTurnFirst":5.8,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180100,"lipSynch":0,"motion":0},{"id":180102,"lipSynch":1,"motion":100}]},{"autoTurnFirst":6,"chara":[{"id":180100},{"id":180102,"motion":200}]},{"autoTurnFirst":2,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180102}]}],"group_16":[{"autoTurnFirst":3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180100,"lipSynch":0,"motion":0,"pos":0,"tear":0,"voice":"vo_char_1801_00_23"},{"cheek":0,"eyeClose":1,"face":"mtn_ex_000.exp3.json","id":180102,"lipSynch":1,"motion":1,"pos":1,"tear":0,"voice":"vo_char_1801_00_23","zOrder":-1}]},{"autoTurnFirst":3.4,"chara":[{"eyeClose":0,"id":180102}]},{"autoTurnFirst":5,"chara":[{"id":180100},{"cheek":0,"face":"mtn_ex_030.exp3.json","id":180102,"motion":300}]},{"autoTurnFirst":2.9,"chara":[{"id":180100},{"eyeClose":1,"id":180102,"motion":0}]},{"autoTurnFirst":3.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180100,"lipSynch":1,"motion":100},{"eyeClose":0,"id":180102,"lipSynch":0}]},{"autoTurnFirst":3.7,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180100},{"id":180102}]}],"group_17":[{"autoTurnFirst":2.9,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":100,"pos":1,"tear":0,"voice":"vo_char_1801_01_24","textHome":"Oh! Thanks for coming!"}]},{"autoTurnFirst":4.5,"chara":[{"id":180101,"motion":1,"textHome":"Kuroe's over at Mitama's getting@an adjustment right now."}]},{"autoTurnFirst":3.7,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180101,"textHome":"I'd be happy if you could keep@me company until she gets back."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_18":[{"autoTurnFirst":1.7,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp3.json","id":180101,"motion":401,"pos":1,"tear":0,"voice":"vo_char_1801_01_25","textHome":"Mmm...! I love the feeling@of sunshine in the morning!"}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":3.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180101}]},{"autoTurnFirst":4.5,"chara":[{"id":180101,"motion":0,"textHome":"There's no sense of time inside the@Magia Record, so greeting each day@like this again is just indescribable."}]},{"autoTurnFirst":5,"chara":[{"eyeClose":1,"id":180101,"motion":301}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_19":[{"autoTurnFirst":5.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":101,"pos":1,"tear":0,"voice":"vo_char_1801_01_26","textHome":"The rest of Mikazuki Villa said@that they'll be having lunch over at@Banbanzai, if you were wondering."}]},{"autoTurnFirst":3.5,"chara":[{"cheek":0,"face":"mtn_ex_041.exp3.json","id":180101,"motion":0,"textHome":"Now I kind of want some@of that Chinese food myself."}]},{"autoTurnFirst":4.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":100,"textHome":"I should invite Kuroe@there next time."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_2":[{"autoTurnFirst":3.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180100,"lipSynch":1,"motion":401,"pos":0,"tear":0,"voice":"vo_char_1801_00_02"},{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":180102,"lipSynch":0,"motion":0,"pos":1,"tear":0,"voice":"vo_char_1801_00_02","zOrder":-1}]},{"autoTurnFirst":3.7,"chara":[{"id":180100,"lipSynch":0,"motion":1},{"id":180102,"lipSynch":1,"motion":300}]},{"autoTurnFirst":4.2,"chara":[{"id":180100},{"id":180102,"motion":200}]},{"autoTurnFirst":2.4,"chara":[{"id":180100},{"eyeClose":1,"id":180102}]}],"group_20":[{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":0,"pos":1,"tear":0,"voice":"vo_char_1801_01_27","textHome":"I think I'm a fan of the@daily dinner prep time."}]},{"autoTurnFirst":3.7,"chara":[{"eyeClose":1,"id":180101,"motion":301,"textHome":"When I imagine how happy everyone@will be to enjoy my delicious meals,@cooking them becomes so much fun."}]},{"autoTurnFirst":3,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_21":[{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_030.exp3.json","id":180101,"motion":200,"pos":1,"tear":0,"voice":"vo_char_1801_01_28","textHome":"When it's just the two of us@at these hours, my mind drifts@towards the same thoughts."}]},{"autoTurnFirst":4.1,"chara":[{"eyeClose":1,"id":180101,"textHome":"About everyone,@about the Magia Record..."}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":5,"chara":[{"id":180101,"motion":300,"textHome":"Things no amount of thinking@will give me right answers for."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_22":[{"autoTurnFirst":2.3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_041.exp3.json","id":180101,"motion":301,"pos":1,"tear":0,"voice":"vo_char_1801_01_29","textHome":"You know, I've also had a time@when I couldn't fit in and found@it hard to go to school."}]},{"autoTurnFirst":3.3,"chara":[{"id":180101}]},{"autoTurnFirst":3.4,"chara":[{"cheek":0,"face":"mtn_ex_030.exp3.json","id":180101,"motion":100,"textHome":"So, well, while I don't claim to fully@understand how Kuroe feels, I do@have some idea of what it's like."}]},{"autoTurnFirst":3,"chara":[{"eyeClose":1,"id":180101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_23":[{"autoTurnFirst":3.2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_030.exp3.json","id":180101,"motion":1,"pos":1,"tear":0,"voice":"vo_char_1801_01_30","textHome":"Now that the APS isn't stable anymore,@I have to make sure we don't revert to@the days when Witches could be born."}]},{"autoTurnFirst":3.5,"chara":[{"eyeClose":1,"id":180101}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":4,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":100,"textHome":"Everyone's been enjoying this peace@of mind, and I want it to continue."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_24":[{"autoTurnFirst":3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp3.json","id":180101,"motion":200,"pos":1,"tear":0,"voice":"vo_char_1801_01_31","textHome":"I wonder how my copies within@Endless Mirrors go about their days."}]},{"autoTurnFirst":2.8,"chara":[{"eyeClose":1,"id":180101}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":3.3,"chara":[{"id":180101,"motion":101,"textHome":"Do you think they get hungry@and sleepy just like I do?"}]},{"autoTurnFirst":3.4,"chara":[{"cheek":0,"face":"mtn_ex_041.exp3.json","id":180101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_25":[{"autoTurnFirst":2,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp3.json","id":180101,"motion":301,"pos":1,"tear":0,"voice":"vo_char_1801_01_32"}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":2.8,"chara":[{"id":180101}]},{"autoTurnFirst":4.8,"chara":[{"id":180101,"motion":0}]},{"autoTurnFirst":3,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180101,"motion":100}]}],"group_26":[{"autoTurnFirst":3.9,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":1,"pos":1,"tear":0,"voice":"vo_char_1801_01_33","textHome":"I've been spending more@time at Mikazuki Villa lately."}]},{"autoTurnFirst":1.4,"chara":[{"cheek":0,"face":"mtn_ex_030.exp3.json","id":180101,"motion":200,"textHome":"She wouldn't tell me why, but@Sana in particular has been sticking@close to me every chance she gets."}]},{"autoTurnFirst":5.5,"chara":[{"id":180101}]},{"autoTurnFirst":4,"chara":[{"eyeClose":1,"id":180101,"motion":300,"textHome":"...I must've made her feel@terribly lonely, haven't I."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_27":[{"autoTurnFirst":1.7,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp3.json","id":180101,"motion":401,"pos":1,"tear":0,"voice":"vo_char_1801_01_34","textHome":"Oh, this? It's a rare Decagon@Ball sticker, apparently."}]},{"autoTurnFirst":3.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180101}]},{"autoTurnFirst":2.4,"chara":[{"id":180101,"motion":100,"textHome":"You can get stronger@just by having it on you."}]},{"autoTurnFirst":4,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180101,"textHome":"I'm supposed to give it to@Kuroe on Felicia's behalf."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_28":[{"autoTurnFirst":2.9,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":400,"pos":1,"tear":0,"voice":"vo_char_1801_01_35","textHome":"Ooh! This rice bowl@keychain is really cute!"}]},{"autoTurnFirst":2,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180101}]},{"autoTurnFirst":3.4,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":1,"textHome":"I wonder if Tsuruno would@be happy to have it."}]},{"autoTurnFirst":1.6,"chara":[{"eyeClose":1,"id":180101,"textHome":"She always works so hard, so@I think she deserves a surprise@gift like this every so often."}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":3.5,"chara":[{"id":180101,"motion":101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_29":[{"autoTurnFirst":2.1,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":200,"pos":1,"tear":0,"voice":"vo_char_1801_01_36","textHome":"Before I became a Magical Girl,@I went to visit Ui at the hospital@almost every single day."}]},{"autoTurnFirst":4.3,"chara":[{"eyeClose":1,"id":180101}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":2.5,"chara":[{"cheek":0,"face":"mtn_ex_030.exp3.json","id":180101,"motion":301,"textHome":"So I guess you could say that I@spent my afterschool time a bit@differently than my classmates."}]},{"autoTurnFirst":3.5,"chara":[{"eyeClose":1,"id":180101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_3":[{"autoTurnFirst":2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180100,"lipSynch":1,"motion":100,"pos":0,"tear":0,"voice":"vo_char_1801_00_03"},{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":180102,"lipSynch":0,"motion":1,"pos":1,"tear":0,"voice":"vo_char_1801_00_03","zOrder":-1}]},{"autoTurnFirst":3,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180100},{"id":180102}]}],"group_30":[{"autoTurnFirst":2.5,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_050.exp3.json","id":180101,"motion":300,"pos":1,"tear":0,"voice":"vo_char_1801_01_37","textHome":"Having fun like a normal middle@schooler is surprisingly hard, huh..."}]},{"autoTurnFirst":2.8,"chara":[{"cheek":0,"face":"mtn_ex_030.exp3.json","id":180101}]},{"autoTurnFirst":4.2,"chara":[{"id":180101,"motion":0,"textHome":"I guess we could go to an@arcade like Rena usually does?"}]},{"autoTurnFirst":3.1,"chara":[{"cheek":0,"face":"mtn_ex_030.exp3.json","id":180101,"motion":200,"textHome":"Though I seem to remember@that costing money... Hmm..."}]},{"autoTurnFirst":3,"chara":[{"eyeClose":1,"id":180101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_31":[{"autoTurnFirst":4.4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":100,"pos":1,"tear":0,"voice":"vo_char_1801_01_38","textHome":"Back when I was fretting about Kuroe,@Yachiyo told me to just \"go see her.\"@It was the nudge I needed, I think."}]},{"autoTurnFirst":2.6,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180101}]},{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_010.exp3.json","id":180101,"motion":301,"textHome":"She must've been worried@about her too, just like I was."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_32":[{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":101,"pos":1,"voice":"vo_char_1801_01_39","textHome":"I never thought I'd ever be able to@relax with Kuroe for a whole day, so@I'm happy we got to have a fun time."}]},{"autoTurnFirst":3.1,"chara":[{"cheek":0,"face":"mtn_ex_011.exp3.json","id":180101}]},{"autoTurnFirst":1.7,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":200,"textHome":"I'd love to invite her over again,@but...I don't want to be a bother."}]},{"autoTurnFirst":2.6,"chara":[{"cheek":0,"face":"mtn_ex_041.exp3.json","id":180101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_33":[{"autoTurnFirst":2,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_030.exp3.json","id":180101,"motion":0,"pos":1,"tear":0,"voice":"vo_char_1801_01_40","textHome":"I was worried that when the Wings@of the Magius disbanded, Kuroe's@hope might've vanished with it."}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":5.7,"chara":[{"id":180101}]},{"autoTurnFirst":4.5,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180101,"motion":301,"textHome":"But after seeing her again, I think@she must've found a new hope now."}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_34":[{"autoTurnFirst":4,"chara":[{"cheek":0,"eyeClose":1,"face":"mtn_ex_030.exp3.json","id":180101,"motion":200,"pos":1,"tear":0,"voice":"vo_char_1801_01_41","textHome":"Ugh... I thought gaining control@over the Magia Record would also@help me change a little, however..."}]},{"autoTurnFirst":0.2,"chara":[{"eyeClose":0,"id":180101}]},{"autoTurnFirst":3.3,"chara":[{"id":180101}]},{"autoTurnFirst":4,"chara":[{"id":180101,"motion":300,"textHome":"I'm not getting any better at@working my smartphone. *Sigh*"}]},{"autoTurnFirst":2,"chara":[{"eyeClose":1,"id":180101}]},{"chara":[{"id":180101,"textHomeStatus":"Clear"}]}],"group_35":[{"autoTurnFirst":2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp3.json","id":180100,"lipSynch":1,"motion":100,"pos":0,"tear":0,"voice":"vo_char_1801_00_42"},{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":180102,"lipSynch":0,"motion":0,"pos":1,"tear":0,"voice":"vo_char_1801_00_42","zOrder":-1}]},{"autoTurnFirst":2.5,"chara":[{"id":180100,"lipSynch":0},{"cheek":0,"face":"mtn_ex_020.exp3.json","id":180102,"lipSynch":1,"motion":200}]}],"group_36":[{"autoTurnFirst":4.4,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_021.exp3.json","id":180100,"lipSynch":1,"motion":200,"pos":0,"tear":0,"voice":"vo_char_1801_00_43"},{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp3.json","id":180102,"lipSynch":0,"motion":0,"pos":1,"tear":0,"voice":"vo_char_1801_00_43","zOrder":-1}]}],"group_37":[{"autoTurnFirst":3.3,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_021.exp3.json","id":180100,"lipSynch":0,"motion":0,"pos":0,"tear":0,"voice":"vo_char_1801_00_44"},{"cheek":0,"eyeClose":0,"face":"mtn_ex_020.exp3.json","id":180102,"lipSynch":1,"motion":400,"pos":1,"tear":0,"voice":"vo_char_1801_00_44","zOrder":-1}]}],"group_38":[{"autoTurnFirst":1.8,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_011.exp3.json","id":180100,"lipSynch":1,"motion":401,"pos":0,"tear":0,"voice":"vo_char_1801_00_45"},{"cheek":0,"eyeClose":0,"face":"mtn_ex_000.exp3.json","id":180102,"lipSynch":0,"motion":0,"pos":1,"tear":0,"voice":"vo_char_1801_00_45","zOrder":-1}]},{"autoTurnFirst":3,"chara":[{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180100,"lipSynch":0,"motion":0},{"cheek":0,"face":"mtn_ex_010.exp3.json","id":180102,"lipSynch":1,"motion":100}]}],"group_39":[{"autoTurnFirst":2,"chara":[{"cheek":0,"eyeClose":0,"face":"mtn_ex_010.